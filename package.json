{
  "name": "pretty-log",
  "version": "1.0.0",
  "description": "Make pretty use of your terminal",
  "main": "./build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --lint",
    "clean:build": "rimraft ./build",
    "clean:src": "rimraft ./src",
    "build": "npm run clean && mkdir build && npm run build:config && npm run build:src && npm run clean:src",
    "build:src": "babel src -d build && babel --presets es2015 env src -d build",
    "build:config": "babel config/index.js -d ./build/config.js && babel --presets es2015 env config/index.js -d ./build/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamuchejude/pretty-log.git"
  },
  "keywords": [
    "console",
    "logo",
    "pretty",
    "warn",
    "info",
    "error",
    "success"
  ],
  "author": {
    "name": "Uche Jude",
    "email": "iamuchejude@gmail.com",
    "url": "https://iamuchejude.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamuchejude/pretty-log/issues"
  },
  "homepage": "https://github.com/iamuchejude/pretty-log#readme",
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "moment": "^2.22.2",
    "npm": "^6.2.0"
  }
}
